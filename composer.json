{
    "name": "jimmitjoo/cart",
    "description": "A simple Laravel Cart.",
    "keywords": ["Laravel", "Cart", "Shopping", "E-commerce"],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "illuminate/database": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Jimmitjoo\\Cart\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jimmitjoo\\Cart\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Jimmie Johansson",
            "email": "jimmitjoo@gmail.com"
        }
    ],
    "scripts": {
        "test": "@php vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jimmitjoo\\Cart\\CartServiceProvider"
            ],
            "aliases": {
                "Cart": "Cart"
            }
        }
    }
}
