{
    "name": "jimmijtoo/cart",
    "description": "A simple Laravel Cart.",
    "require": {
        "php": "^8.0",
        "illuminate/database": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Jimmijtoo\\Cart\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Jimmie Johansson",
            "email": "jimmitjoo@gmail.com"
        }
    ],
    "scripts": {
        "test": "@php vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jimmitjoo\\Cart\\CartServiceProvider"
            ],
            "aliases": {
                "Cart": "Cart"
            }
        }
    }
}
